{
	"name": "enkel",
	"displayName": "enkel",
	"description": "Dev Tools",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.46.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:enkel.greeting",
		"onCommand:enkel.edit"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "enkel.greeting",
				"title": "Enkel Greeting"
			},
			{
				"command": "enkel.edit",
				"title": "Enkel Edit"
			}
		],
		"keybindings": [
			{
				"command": "enkel.greeting",
				"key": "ctrl+k",
				"mac": "cmd+k",
				"when": "editorTextFocus"
			}
		],
		"menus": {
			"editor/context": [
				{
					"when": "editorFocus",
					"command": "enkel.greeting",
					"group": "navigation"
				},
				{
					"when": "editorFocus",
					"command": "enkel.edit",
					"group": "navigation"
				}
			],
			"explorer/context": [
				{
					"command": "enkel.greeting",
					"group": "navigation"
				}
			]
		}
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.46.0",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.2",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	}
}